  <form method="POST" action="/logout"> 
  <input type="submit" value="Logout">
<% } else { %>
<br><br>
 
  <a href="/register" input id="register" type="text" name="register"><font size="6"> Register </a> </font>
<br><br> 
  <a href="/login" input id="login" type="text" name="login"><font size="6">  Log In  </a> </font>
<% } %>
</form> 
</header>